
public class SuperClass {

	private double north = 0;
	private double south = 0;
	private double west = 0;
	private double east = 0;
	

	public void setNorth(double north) {
		this.north = this.north + north;
	}
	public void setEast(double east) {
		this.east = this.east + east;
	}
	public void setWest(double west) {
		this.west = this.west + west;
	}
	public void setSouth(double south) {
		this.south = this.south + south;
	}
	
	public double getTallyNorth() {
		return (this.north - this.south);
	}
	public double getTallyEast() {
		return this.east - this.west;
	}
	public double getNorth() {
		return this.north;
	}
	public double getEast() {
		return this.east;
	}
	public double getWest() {
		return this.west;
	}
	public double getSouth() {
		return this.south;
	}
}
	

